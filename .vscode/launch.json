    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_app_flavors",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_app_flavors (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"


        },
        {
            "name": "Flutter Dev",
            "request": "launch",
            "type": "dart",
            "args": [
                "-t",
                "lib/main_dev.dart",
                "--flavor",
                "dev"
            ]
          },
          {
            "name": "Flutter QA",
            "request": "launch",
            "type": "dart",
            "args": [
                "-t",
                "lib/main_qa.dart",
                "--flavor",
                "qa"
            ]
          },
          {
            "name": "Flutter Prod",
            "request": "launch",
            "type": "dart",
            "args": [
                "-t",
                "lib/main_prod.dart",
                "--flavor",
                "prod"
            ]
          }

    ]
}